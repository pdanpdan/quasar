  empty-cells show

  thead
    text-align left
    vertical-align bottom
    font-weight 700

  th, td
    &:first-child
      border-left-width 0

    margin 0
    overflow visible
    padding .5rem .7rem

  &.compact
    th, td
      padding .1rem .3rem
  &.loose
    th, td
      padding 1rem

  &.bordered, &.cell-delimiter:not(.responsive):not(.flipped)
    border $table-border

  &:not(.responsive):not(.flipped)
    &.horizontal-delimiter, &.cell-delimiter
      th, td
        border-bottom $table-border

