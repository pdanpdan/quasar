{
  "type": "component",
  "props": {
    "color": {
      "type": "String",
      "desc": "Color name for component from the Quasar Color Palette",
      "examples": [
        "primary",
        "teal-10"
      ]
    },
    "responsive": {
      "type": "Boolean",
      "desc": "Use responsive mode"
    },
    "dark": {
      "type": "Boolean",
      "desc": "Notify the component that the background is a dark color"
    }
  },
  "slots": {
    "default": {
      "desc": "Used for content of component"
    }
  }
}