{
  "type": "component",
  "props": {
    "name": {
      "type": [
        "Number",
        "String"
      ],
      "desc": "Panel name",
      "required": true,
      "examples": [
        "accounts",
        "firstPanel",
        ":name=\"1\""
      ]
    },
    "disable": {
      "type": "Boolean",
      "desc": "Put component in disabled mode"
    }
  },
  "slots": {
    "default": {
      "desc": "Default slot in the devland unslotted content of the component"
    }
  }
}